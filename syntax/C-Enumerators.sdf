module C-Enumerators
exports
  context-free syntax
    "enum" IdOrTypedefName? "{" {Enumerator ","}+ "}" -> EnumSpecifier {cons("C-Enum")}
    "enum" IdOrTypedefName                            -> EnumSpecifier {cons("C-EnumId")}

  context-free syntax
    IdOrTypedefName EnumeratorValue? -> Enumerator {cons("C-Enumerator")}

  context-free syntax
    "=" ConstantExp -> EnumeratorValue {cons("C-EnumVal")}

